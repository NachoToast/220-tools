{
    "name": "220-tools",
    "version": "1.0.0",
    "homepage": "https://220.nachotoast.com",
    "repository": {},
    "private": true,
    "license": "MIT",
    "scripts": {
        "build": "webpack --config webpack.config.js --mode production",
        "dev": "nodemon",
        "lint": "eslint --ext .ts --max-warnings=0",
        "typecheck": "tsc --noEmit",
        "check-all": "yarn lint && yarn typecheck"
    },
    "devDependencies": {
        "@types/node": "^17.0.32",
        "@typescript-eslint/eslint-plugin": "^5.23.0",
        "@typescript-eslint/parser": "^5.23.0",
        "eslint": "^8.15.0",
        "eslint-config-prettier": "^8.5.0",
        "nodemon": "^2.0.16",
        "prettier": "^2.6.2",
        "ts-loader": "^9.3.0",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.4",
        "webpack": "^5.72.1",
        "webpack-cli": "^4.9.2"
    },
    "nodemonConfig": {
        "watch": [
            "src"
        ],
        "ext": "ts",
        "exec": "yarn build"
    }
}
