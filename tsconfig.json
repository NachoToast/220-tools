{
    "compilerOptions": {
        "target": "ES5",
        "module": "ES6",
        "sourceMap": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "strict": true,
        "alwaysStrict": true,
        "types": ["node"],
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true
    },
    "include": ["src/**/*", "hash-tables"]
}
